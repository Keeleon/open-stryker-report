{
  "name": "open-stryker-report",
  "displayName": "open-stryker-report",
  "description": "Opens html reports generated by Stryker in your default browser.",
  "version": "0.0.6",
  "publisher": "craig-simko",
  "repository": {
    "type": "git",
    "url": "https://github.com/Keeleon/open-stryker-report.git"
  },
  "icon": "icon/open-stryker-report.png",
  "engines": {
    "vscode": "^1.22.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.openStrykerReport",
    "workspaceContains:stryker.conf.js"
  ],
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.65",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3",
    "vscode": "^1.1.18"
  },
  "dependencies": {
    "@types/fs-extra": "^5.0.2",
    "@types/open": "0.0.29",
    "fs-extra": "^5.0.0",
    "open": "0.0.5"
  }
}
